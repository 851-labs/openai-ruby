module OpenAI
  module Models
    module Audio
      type translation = { text: String }

      class Translation < OpenAI::BaseModel
        attr_accessor text: String

        def initialize:
          (text: String) -> void
          | (
            ?OpenAI::Models::Audio::translation | OpenAI::BaseModel data
          ) -> void

        def to_hash: -> OpenAI::Models::Audio::translation
      end
    end
  end
end
